{
  "name": "perf-tpl-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/update-sw-version.js",
    "dev": "vite",
    "prebuild": "node scripts/update-sw-version.js",
    "build": "tsc && vite build",
    "build:wasm": "cd src/wasm && cargo build --release --target wasm32-unknown-unknown && wasm-bindgen target/wasm32-unknown-unknown/release/wasm_compute.wasm --out-dir ../../public/wasm --target web",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives",
    "seo:sitemap": "node scripts/generate-sitemap.js",
    "seo:analyze": "node scripts/seo-analyzer.js"
  },
  "dependencies": {
    "idb": "^8.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-react": "^5.1.4",
    "chrome-launcher": "^1.2.1",
    "eslint": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "globals": "^17.3.0",
    "lighthouse": "^13.0.3",
    "rollup-plugin-visualizer": "^7.0.0",
    "sitemap": "^9.0.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "workbox-window": "^7.0.0"
  }
}